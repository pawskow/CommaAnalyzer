match_rules(
	apply(
		match(
			and(inter(cas[0], {voc}), inter(class[0], {subst}))

		),
		actions(
			mark(M, "WolaczRzeczownik")
		)
	);

	apply(
		match(
			inter(class[-1], {praet, fin, impt}),
			repeat(
				not(inter(class[0], {praet, fin, interp, impt}))
			)
		),
		actions(
			mark(M, "PodzdanieZCzasownikiemNaPoczatku")
		)
	)

)



